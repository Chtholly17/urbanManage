

/* pages/add_space/add_space.wxss */ 
 
.wrap {  
    
  width: 100%; 
  padding: 0 30rpx; 
  box-sizing: border-box; 
} 
.img-wrap {  
    
  font-size: 30rpx; 
  color: #33373E; 
  margin-bottom: 10rpx; 
} 
.txt {  
    
  margin-bottom: 20rpx; 
} 
.imglist {  
    
  display: flex; 
  flex-wrap: wrap; 
} 
.item {  
    
  width: 150rpx; 
  height: 150rpx; 
  margin-right: 22rpx; 
  margin-bottom: 10rpx; 
  position: relative; 
} 
.last-item {  
    
  width: 150rpx; 
  height: 150rpx; 
  text-align: center; 
  line-height: 146rpx; 
  border: 2rpx dashed #8B97A9; 
  box-sizing: border-box; 
} 
.item image {  
    
  width: 100%; 
  height: 100%; 
} 
.delete {  
    
  width: 30rpx; 
  height: 30rpx; 
  position: absolute; 
  top: -14rpx; 
  right: -12rpx; 
} 
 
.reset { 
  color: white; 
  background: #353535; 
} 
 
.save { 
  color: black; 
  background: white 
} 
 
.footer { 
  width: 100%; 
  height: 10vh; 
  position: fixed; 
  bottom: 0; 
  display: flex; 
  border-top: lightgray solid 1px; 
} 
 
.footer view { 
  width: 50%; 
  height: 100%; 
  padding-top: 2vh; 
  text-align: center; 
}